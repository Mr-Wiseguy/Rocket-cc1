xgcc.exe : gcc.o version.o choose-temp.o pexecute.o prefix.o version.o     $(LIBDEPS) $(EXTRA_GCC_OBJS)
	link $(LDFLAGS) -out:$@ gcc.o prefix.o version.o  	  choose-temp.o pexecute.o $(EXTRA_GCC_OBJS) $(LIBS)

